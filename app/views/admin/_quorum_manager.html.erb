<%= javascript_include_tag 'page_specific/quorum_manager' %>

<table>
  <thead>
    <tr>
      <th>チーム</th>
      <!-- ko foreach: periods -->
        <th data-bind="text: time"></th>
      <!-- /ko -->
    </tr>
  </thead>
  <tbody data-bind="foreach: { data: teams, as: 'team' }">
    <tr>
      <th data-bind="text: name"></th>
      <!-- ko foreach: { data: $parent.periods, as: 'period' } -->
        <td data-bind="text: team.quorum(period)"></td>
      <!-- /ko -->
    </tr>
  </tbody>
</table>
